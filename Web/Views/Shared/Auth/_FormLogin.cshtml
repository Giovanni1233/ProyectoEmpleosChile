<div class="col-12 col-sm-12 col-md-12 col-lg-12">
    <div class="row">
        <div class="col-12 col-sm-12 col-md-12 col-lg-12" id="errorSignIn"></div>

        <div class="col-12 col-sm-12 col-md-12 col-lg-12">
            <div class="md-form form-sm">
                <i class="fas fa-user prefix" style="margin-top: 4px;"></i>
                <input type="text" id="username" class="form-control" autocomplete="off" maxlength="50">
                <label for="username">Usuario (Rut o Correo)</label>
            </div>
        </div>
        <div class="col-12 col-sm-12 col-md-12 col-lg-12" style="margin-top: -30px;">
            <div class="md-form form-sm">
                <i class="fas fa-key prefix" style="margin-top: 4px;"></i>
                <input type="password" id="userPassword" class="form-control" maxlength="6">
                <label for="userPassword">Contraseña</label>
            </div>
        </div>
        <div class="col-12 col-sm-12 col-md-12 col-lg-12" style="text-align: center;">
            <button id="signInUser" class="btn btn-primary btn-block">Ingresar</button>
        </div>

        <div class="col-12 col-sm-12 col-md-12 col-lg-12" style="margin-top:5px;">
            <p class="hint-text">
                <a href="#">Olvide mi contraseña?</a>
            </p>
        </div>
    </div>
</div>

